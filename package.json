    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-rollup-preprocessor": "^7.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-safaritechpreview-launcher": "^2.0.2",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "ncat": "^2.0.5",
